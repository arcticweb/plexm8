{
  "name": "plexm8",
  "version": "1.0.0",
  "description": "A smart playlist manager for Plex Media Server",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/generate-manifest.mjs && tsc && vite build",
    "build:prod": "cross-env VITE_APP_BASE_PATH=/plexm8/ npm run build",
    "build:staging": "cross-env VITE_APP_BASE_PATH=/plexm8-staging/ npm run build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "format": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@netlify/functions": "^5.1.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "cross-env": "^7.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.24",
    "prettier": "^3.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
